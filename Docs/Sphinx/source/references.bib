@misc{Kogelschatz2003,
abstract = {Dielectric-barrier discharges (silent discharges) are used on a large industrial scale. They combine the ad{\^{u}}antages of non-equilibrium plasma properties with the ease of atmospheric-pressure operation. A prominent feature is the simple scalability from small laboratory reactors to large industrial installations with megawatt input powers. Efficient and cost-effecti{\^{u}}e all-solid-state power supplies are a{\^{u}}ailable. The preferred frequency range lies between 1 kHz and 10 MHz, the preferred pressure range between 10 kPa and 500 kPa. Industrial applications include ozone gener- ation, pollution control, surface treatment, high power CO 2 lasers, ultra{\^{u}}iolet excimer lamps, excimer based mercury-free fluorescent lamps, and flat large-area plasma displays. Depending on the application and the operating conditions the discharge can ha{\^{u}}e pronounced filamentary structure or fairly diffuse appearance. History, discharge physics, and plasma chemistry of dielectric-barrier discharges and their applications are discussed in detail.},
archivePrefix = {arXiv},
arxivId = {arXiv:astro-ph/0005074v1},
author = {Kogelschatz, Ulrich},
booktitle = {Plasma Chemistry and Plasma Processing},
doi = {10.1023/A:1022470901385},
eprint = {0005074v1},
isbn = {1009982220290},
issn = {02724324},
keywords = {CO2 lasers,Dielectric-barrier discharges,Excimer lamps,Non-equilibrium plasmas,Ozone synthesis,Plasma displays,Pollution control,Silent discharges,Surface treatment},
pmid = {1284},
primaryClass = {arXiv:astro-ph},
title = {{Dielectric-barrier discharges: Their History, Discharge Physics, and Industrial Applications}},
year = {2003}
}
@article{Spiteri2002,
abstract = {Strong-stability-preserving (SSP) time discretization methods have$\backslash$na nonlinear sta- bility property that makes them particularly suitable$\backslash$nfor the integration of hyperbolic conservation laws where discontinuous$\backslash$nbehaviour is present. Optimal SSP schemes have been previously found$\backslash$nfor methods of order 1, 2, and 3, where the number of stages s equals$\backslash$nthe order p. An optimal low-storage SSP scheme with s: p: 3 is also$\backslash$nknown. In this paper, we present a new class of optimal high-order$\backslash$nSSP and low-storage SSP Runge-Kutta schemes with s $\backslash${\textgreater} p. We find$\backslash$nthat these schemes are ultimately more efficient than the known schemes$\backslash$nwith s: p because the increase in the allowable time step more than$\backslash$noffsets the added computational expense per step. We demonstrate$\backslash$nthese efficiencies on a set of representative problems from compressible$\backslash$ngas flows.},
