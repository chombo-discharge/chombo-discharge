@article{doi:10.1002/ente.201500127,
abstract = {Abstract The use of plasma technology for CO2 splitting is gaining increasing interest, but one of the major obstacles to date for industrial implementation is the considerable energy cost. We demonstrate that the introduction of a packing of dielectric zirconia (ZrO2) beads into a dielectric barrier discharge (DBD) plasma reactor can enhance the CO2 conversion and energy efficiency up to a factor 1.9 and 2.2, respectively, compared to that in a normal (unpacked) DBD reactor. We obtained a maximum conversion of 42 {\%} and a maximum energy efficiency of 9.6 {\%}. However, it is the ability of the packing to almost double both the conversion and the energy efficiency simultaneously at certain input parameters that makes it very promising. The improved conversion and energy efficiency can be explained by the higher values of the local electric field and electron energy near the contact points of the beads and the lower breakdown voltage, demonstrated by 2 D fluid modeling.},
author = {{Van Laer}, Koen and Bogaerts, Annemie},
doi = {10.1002/ente.201500127},
journal = {Energy Technology},
keywords = {carbon dioxide,computational chemistry,energy conversion,plasma chemistry,sustainable chemistry},
number = {10},
pages = {1038--1044},
title = {{Improving the Conversion and Energy Efficiency of Carbon Dioxide Splitting in a Zirconia-Packed Dielectric Barrier Discharge Reactor}},
url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/ente.201500127},
volume = {3},
year = {2015}
}
@article{Akiyama2000,
abstract = {Industrial applications using pulsed power technologies have been$\backslash$ndeveloped in many fields. One of them is the treatment of exhaust gas by$\backslash$nusing uniformly produced streamer discharges. Streamer discharges in$\backslash$nliquid also generate extremely high electric fields at the tip of$\backslash$nstreamers, as well as high energy electrons, ozone, other chemically$\backslash$nactive species, ultraviolet rays and shock waves. All of these may be$\backslash$nutilized to sterilize microorganisms and to decompose molecules and$\backslash$nmaterials. Large-volume streamer discharges in water have been produced$\backslash$nin order to realize industrial applications. A wire to plane electrode$\backslash$nconfiguration has been used. The discharge in water shows similar$\backslash$noptical radiation to that in an atmospheric gas as provided by evidence$\backslash$nfrom still photographs. The influence of polarity, conductivity,$\backslash$nelectrode geometry and hydrostatic pressure on the streamers in liquid,$\backslash$nthe mechanism of streamer discharges and possible industrial$\backslash$napplications of streamers in liquids, are discussed in the present work$\backslash$n},
