# ====================================================================================================
# TracerParticleSolver class options
# ====================================================================================================
TracerParticleSolver.deposition   = cic    # Deposition method. Must be 'ngp', 'cic', 'tsc'
TracerParticleSolver.plot_scalars = true   # Turn on/off plotting of the particle scalars
TracerParticleSolver.plot_vectors = true   # Turn on/off plotting of the particle vectors