# ====================================================================================================
# AIR7_ZHELEZNYAK CLASS OPTIONS
# ====================================================================================================	

# ----------------------------------------------------------------------------------------------------	
# Transport file and # of points to use when converting files to uniform spacings
# ----------------------------------------------------------------------------------------------------
CdrPlasmaAir7Zheleznyak.transport_file      = transport_data.txt          # Transport file
CdrPlasmaAir7Zheleznyak.uniform_tables      = 100	                   #  Converted # of points in transport file

# ----------------------------------------------------------------------------------------------------	
# Gas composition. This should match your BOLSIG stuff!
# ----------------------------------------------------------------------------------------------------
CdrPlasmaAir7Zheleznyak.pressure            = 1.0                         # Gas pressure
CdrPlasmaAir7Zheleznyak.temperature         = 300                         # Gas temperature
CdrPlasmaAir7Zheleznyak.frac_N2             = 0.8                         # N2 fraction
CdrPlasmaAir7Zheleznyak.frac_O2             = 0.2                         # O2 fraction

# ----------------------------------------------------------------------------------------------------	
# Transport properties
# ----------------------------------------------------------------------------------------------------
CdrPlasmaAir7Zheleznyak.use_alpha_corr      = false                       # Soloviev alpha correction	
CdrPlasmaAir7Zheleznyak.mobile_electrons    = true                        # Mobile Electrons or not
CdrPlasmaAir7Zheleznyak.diffusive_electrons = true                        # Diffusive Electrons or not
CdrPlasmaAir7Zheleznyak.mobile_ions         = false                       # Mobile ions or not
CdrPlasmaAir7Zheleznyak.diffusive_ions      = false                       # Diffusive ions or not
CdrPlasmaAir7Zheleznyak.ion_mobility        = 2.E-4                       # Ion mobility

# ----------------------------------------------------------------------------------------------------	
# Chemistry algorithm
# ----------------------------------------------------------------------------------------------------
CdrPlasmaAir7Zheleznyak.chemistry_algorithm = rk2  # 'euler', 'rk2', or 'rk4'
CdrPlasmaAir7Zheleznyak.chemistry_dt        = 1.0  # Minimum dt used for chemistry

# ----------------------------------------------------------------------------------------------------
# RNG stuff
# ----------------------------------------------------------------------------------------------------
CdrPlasmaAir7Zheleznyak.rng_seed            = 0    # RNG seed. < 0 gives a random seed
CdrPlasmaAir7Zheleznyak.poiss_exp_swap      = 100  # Swap from Poisson to exponential distribution

# ----------------------------------------------------------------------------------------------------
# Initial data
# ----------------------------------------------------------------------------------------------------
CdrPlasmaAir7Zheleznyak.uniform_density       = 1.E4         # Uniform density
CdrPlasmaAir7Zheleznyak.seed_density          = 1.E18        # Gaussian seed density
CdrPlasmaAir7Zheleznyak.seed_radius           = 100E-6       # Gaussian seed radius
CdrPlasmaAir7Zheleznyak.seed_position         = 0.0 0.0 0.0  # Gaussian seed position

# ----------------------------------------------------------------------------------------------------	
# SEE coefficients
# ----------------------------------------------------------------------------------------------------
CdrPlasmaAir7Zheleznyak.electrode_townsend2           = 1.E-6             # SEE on electrodes
CdrPlasmaAir7Zheleznyak.dielectric_townsend2          = 1.E-6             # SEE on dielectrics
CdrPlasmaAir7Zheleznyak.electrode_quantum_efficiency  = 1.E-6             # SEE on electrodes
CdrPlasmaAir7Zheleznyak.dielectric_quantum_efficiency = 1.E-6             # SEE on dielectrics

# ----------------------------------------------------------------------------------------------------	
# Exitation efficiencies, photoionization efficiences, quenching pressure, radiative lifetimes,
# and predissociation lifetime, and absorption lengths. These are pressure independent.
# ----------------------------------------------------------------------------------------------------
CdrPlasmaAir7Zheleznyak.quenching_pressure            = 0.03947           # Quenching pressure for photo-emission (atm)
CdrPlasmaAir7Zheleznyak.photoi_factor                 = 1.0               # Photoionization factor
CdrPlasmaAir7Zheleznyak.photoi_seed                   = 0                 # RNG seed
CdrPlasmaAir7Zheleznyak.photoi_f1                     = 2.925E15          # Lower frequency
CdrPlasmaAir7Zheleznyak.photoi_f2                     = 3.059E15          # High frequency
CdrPlasmaAir7Zheleznyak.photoi_K1                     = 2.65E-2           # Converted to (mPa)^-1
CdrPlasmaAir7Zheleznyak.photoi_K2                     = 1.5               # Converted to (mPa)^-1
