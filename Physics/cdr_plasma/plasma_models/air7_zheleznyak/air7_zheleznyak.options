# ====================================================================================================
# AIR7_ZHELEZNYAK CLASS OPTIONS
# ====================================================================================================	

# ----------------------------------------------------------------------------------------------------	
# Transport file and # of points to use when converting files to uniform spacings
# ----------------------------------------------------------------------------------------------------
air7_zheleznyak.transport_file      = transport_data.txt          # Transport file
air7_zheleznyak.uniform_tables      = 100	                   #  Converted # of points in transport file

# ----------------------------------------------------------------------------------------------------	
# Gas composition. This should match your BOLSIG stuff!
# ----------------------------------------------------------------------------------------------------
air7_zheleznyak.pressure            = 1.0                         # Gas pressure
air7_zheleznyak.temperature         = 300                         # Gas temperature
air7_zheleznyak.frac_N2             = 0.8                         # N2 fraction
air7_zheleznyak.frac_O2             = 0.2                         # O2 fraction

# ----------------------------------------------------------------------------------------------------	
# Transport properties
# ----------------------------------------------------------------------------------------------------
air7_zheleznyak.use_alpha_corr      = false                       # Soloviev alpha correction	
air7_zheleznyak.mobile_electrons    = true                        # Mobile electrons or not
air7_zheleznyak.diffusive_electrons = true                        # Diffusive electrons or not
air7_zheleznyak.mobile_ions         = false                       # Mobile ions or not
air7_zheleznyak.diffusive_ions      = false                       # Diffusive ions or not
air7_zheleznyak.ion_mobility        = 2.E-4                       # Ion mobility

# ----------------------------------------------------------------------------------------------------	
# Chemistry algorithm
# ----------------------------------------------------------------------------------------------------
air7_zheleznyak.chemistry_algorithm = rk2  # 'euler', 'rk2', or 'rk4'
air7_zheleznyak.chemistry_dt        = 1.0  # Minimum dt used for chemistry

# ----------------------------------------------------------------------------------------------------
# RNG stuff
# ----------------------------------------------------------------------------------------------------
air7_zheleznyak.rng_seed            = 0    # RNG seed. < 0 gives a random seed
air7_zheleznyak.poiss_exp_swap      = 100  # Swap from Poisson to exponential distribution

# ----------------------------------------------------------------------------------------------------
# Initial data
# ----------------------------------------------------------------------------------------------------
air7_zheleznyak.uniform_density       = 1.E4         # Uniform density
air7_zheleznyak.seed_density          = 1.E18        # Gaussian seed density
air7_zheleznyak.seed_radius           = 100E-6       # Gaussian seed radius
air7_zheleznyak.seed_position         = 0.0 0.0 0.0  # Gaussian seed position

# ----------------------------------------------------------------------------------------------------	
# SEE coefficients
# ----------------------------------------------------------------------------------------------------
air7_zheleznyak.electrode_townsend2           = 1.E-6             # SEE on electrodes
air7_zheleznyak.dielectric_townsend2          = 1.E-6             # SEE on dielectrics
air7_zheleznyak.electrode_quantum_efficiency  = 1.E-6             # SEE on electrodes
air7_zheleznyak.dielectric_quantum_efficiency = 1.E-6             # SEE on dielectrics

# ----------------------------------------------------------------------------------------------------	
# Exitation efficiencies, photoionization efficiences, quenching pressure, radiative lifetimes,
# and predissociation lifetime, and absorption lengths. These are pressure independent.
# ----------------------------------------------------------------------------------------------------
air7_zheleznyak.quenching_pressure            = 0.03947           # Quenching pressure for photo-emission (atm)
air7_zheleznyak.photoi_factor                 = 1.0               # Photoionization factor
air7_zheleznyak.photoi_seed                   = 0                 # RNG seed
air7_zheleznyak.photoi_f1                     = 2.925E15          # Lower frequency
air7_zheleznyak.photoi_f2                     = 3.059E15          # High frequency
air7_zheleznyak.photoi_K1                     = 2.65E-2           # Converted to (mPa)^-1
air7_zheleznyak.photoi_K2                     = 1.5               # Converted to (mPa)^-1
